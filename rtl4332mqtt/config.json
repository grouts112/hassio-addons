{
  "name": "RTL_433 to MQTT Bridge - grouts112",
  "version": "0.2.7",
  "slug": "rtl4332mqtt",
  "description": "433MHz RF to MQTT Bridge based on RTL_SDR/RTL_433 for RTL2832U based DVB-T USB tuners",
  "url": "https://github.com/grouts112/hassio-addons/edit/master/rtl4332mqtt/",
  "boot": "auto",
  "startup": "services",
  "map": ["config:rw", "ssl"],
  "usb": true,
  "host_network": "False",
  "init": "false",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "options":
  {
    "mqtt_host": "hassio.local",
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "homeassistant/tele/RF433/sensor",
    "protocol": 109,
    "frequency": 433920000,
    "gain": 90,
    "frequency_offset": 350
  },
  "schema":
  {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str",
    "protocol": "int",
    "frequency": "int",
    "gain": "int",
    "frequency_offset": "int"
   }
}
